{
  "name": "app-store",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --configuration production --base-href /app-store/ --deploy-url /app-store/ --output-path dist/app/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format-only": "prettier --write \"src/**/*.{ts,html,scss,css,json}\"",
    "format": "eslint --fix \"src/**/*.ts\" && prettier --write \"src/**/*.{ts,html,scss,css,json}\"",
    "format:check": "eslint \"src/**/*.ts\" && prettier --check \"src/**/*.{ts,html,scss,css,json}\"",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "serve-dev": "ng serve --configuration development --host $HOSTNAME --ssl"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "dependencies": {
    "@angular/animations": "21.0.2",
    "@angular/cdk": "21.0.1",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "21.0.1",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/service-worker": "^21.0.0",
    "@fontsource-variable/material-symbols-outlined": "^5.2.30",
    "@fontsource/roboto": "^5.2.9",
    "angular-auth-oidc-client": "21.0.0",
    "lodash-es": "^4.17.21",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.0.1",
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.0",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/parser": "^8.48.1",
    "angular-eslint": "21.0.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.3",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.4",
    "vitest": "^4.0.8"
  }
}
